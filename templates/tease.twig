<article class="tease tease-{{post.post_type}}" id="tease-{{post.ID}}" style="padding-left:50px; padding-top: 10px">
	{% block content %}
		<h2 class="h2"><a href="{{post.link}}">{{post.title}}</a></h2>
		<p>{{post.preview}}</p>
		{% if post.get_thumbnail %}
			<img src="{{post.thumbnail.src}}" />
		{% endif %}
	{% endblock %}
</article>
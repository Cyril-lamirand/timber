{% if menu %}

    <ul class="navbar-nav">
	{% for item in items %}
      <li class="{{ item.classes | join(' ') }}">
        <a target="{{ item.target }}" href="{{ item.link }}" style="margin-left : 10px;">{{ item.title }}</a>
      {% include "menu.twig" with {'items': item.children} %}
		</li>
	{% endfor %}
      
    </ul>


{% endif %}
